{
    "name": "stethome/ory-auth-bundle",
    "license": "MIT",
    "description": "Symfony Authenticator for Ory Kratos",
    "keywords": ["Symfony", "bundle", "Ory", "Kratos", "authentication"],
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "StethoMe",
            "homepage": "https://stethome.com/"
        },
        {
            "name": "Filip Maciejewski",
            "email": "maciejewski@stethome.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "phpunit": "vendor/bin/simple-phpunit --testdox"
    },
    "require": {
        "php": ">=8.1",
        "ory/kratos-client-php": "^1.0",
        "symfony/http-kernel": "^5.4|^6.3|^7.0",
        "symfony/security-bundle": "^5.4|^6.3|^7.0"
    },
    "require-dev": {
        "symfony/console": "^5.4|^6.3|^7.0",
        "symfony/phpunit-bridge": "^5.4|^6.3|^7.0",
        "symfony/stopwatch": "^5.4|^6.3|^7.0"
    },
    "autoload": {
        "psr-4": {
            "StethoMe\\OryAuthBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "StethoMe\\OryAuthBundle\\Tests\\": "tests"
        }
    }

}
